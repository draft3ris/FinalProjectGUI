<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" prefHeight="562.0" prefWidth="551.0" spacing="20.0" style="-fx-background-color: pink;" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.minigame.UpDownController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <ImageView fx:id="upDownScreen" fitHeight="416.0" fitWidth="428.0" pickOnBounds="true" preserveRatio="true">
      <image>
         <Image url="@../../../../../../up.jpeg" />
      </image></ImageView>
   <HBox prefHeight="27.0" prefWidth="358.0">
      <children>
         <TextField fx:id="inputField" prefHeight="26.0" prefWidth="305.0">
            <HBox.margin>
               <Insets left="80.0" />
            </HBox.margin></TextField>
         <Button fx:id="submitButton" mnemonicParsing="false" onMouseClicked="#onSubmitButtonClick" prefHeight="26.0" prefWidth="78.0" text="Submit" />
      </children>
   </HBox>
   <Button fx:id="homeButton" mnemonicParsing="false" onMouseClicked="#onHomeButtonClicked" prefHeight="26.0" prefWidth="119.0" text="back to home">
      <font>
         <Font size="16.0" />
      </font>
   </Button>
</VBox>
